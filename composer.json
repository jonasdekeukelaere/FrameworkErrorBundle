{
  "name": "sumocoders/framework-error-bundle",
  "type": "symfony-bundle",
  "description": "Error bundle for our mini-framework",
  "keywords": ["Errbit", "SumoCoders"],
  "license": "MIT",
  "authors": [
    {
      "name": "Tijs Verkoyen",
      "email": "tijs@sumocoders.be"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^5.5 || ^7.0",

    "eo/airbrake-bundle": "dev-master as 1.1"
  },
  "require-dev": {
    "symfony/symfony": "^2.8",
    "symfony/framework-bundle": "^2.8",
    "symfony/monolog-bundle": "^2.8",
    "sensio/framework-extra-bundle": "^3.0",
    "symfony/phpunit-bridge": "^3.0",
    "phpunit/phpunit": "^5.2"
  },
  "autoload": {
    "psr-0": { "SumoCoders\\FrameworkErrorBundle": "" }
  },
  "target-dir": "SumoCoders/FrameworkErrorBundle"
}
