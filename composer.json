{
  "name": "sumocoders/framework-error-bundle",
  "type": "symfony-bundle",
  "description": "Error bundle for our mini-framework",
  "keywords": ["Errbit", "SumoCoders"],
  "license": "MIT",
  "authors": [
    {
      "name": "Tijs Verkoyen",
      "email": "tijs@sumocoders.be"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.4",
    "eo/airbrake-bundle": "~1.0@dev"
  },
  "require-dev": {
    "symfony/symfony": "2.6.*",
    "symfony/framework-bundle": "~2.6",
    "symfony/monolog-bundle": "~2.6",
    "sensio/framework-extra-bundle": "~3.0",
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-0": { "SumoCoders\\FrameworkErrorBundle": "" }
  },
  "target-dir": "SumoCoders/FrameworkErrorBundle"
}
